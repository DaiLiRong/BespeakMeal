<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
   assembly="BespeakMeal.Domain"
   namespace="BespeakMeal.Domain.Entities">

  <class name ="Food" table="tbl_food">
    <id name="FoodId" column ="FoodId">
      <generator class ="native"/>
    </id>
    <property name ="FoodName"/>
    <property name ="FoodPrice"/>
    <property name ="FoodType"/>
    <property name ="Status"/>
    <property name ="FoodContent"/>
  </class>

</hibernate-mapping>